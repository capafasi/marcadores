<p id="notice"><%= notice %></p>

<h1>Bookmarks</h1>

<%= pie_chart @pie_kinds, title: "Bookmarks by Kind"%>

<br>

<%= render 'form', bookmark: @bookmark %>

<table class="table table-bordered" id="table_bookmarks">
  <thead>
    <tr>
      <th>Name</th>
      <th>Url</th>
      <th>Category</th>
      <th>Kind</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= render @bookmarks %>
  </tbody>
</table>

<br>



<%= link_to 'New Bookmark', new_bookmark_path %>
